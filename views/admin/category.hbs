<main class="main-wrap">

        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Category List</h2>

                </div>
                <div>
                    <a href="/admin/add_category" class="btn btn-primary btn-sm rounded">Add Category</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            <div class="form-check ms-2">
                                <input class="form-check-input" type="checkbox" value="">
                            </div>
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <select class="form-select">
                                <option selected>All category</option>
                                <option>Electronics</option>
                                <option>Clothes</option>
                                <option>Automobile</option>
                            </select>
                        </div>
                        <div class="col-md-2 col-6">
                            <input type="date" value="02.05.2021" class="form-control">
                        </div>
                        <div class="col-md-2 col-6">
                            <select class="form-select">
                                <option selected>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                   
               <!---category tab-->
           {{#each allCtegoryData}}
                    <article class="itemlist">
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="">
                                </div>
                            </div>
                            <div class="col-lg-3 col-sm-3 col-3 flex-grow-1 col-name jsutify-content-center text-align-center">
                                <a class="itemside" href="#">
                                    <div class="d-flex aligin-items-center justify-content-center">
                                        <img src="/images/products/{{this.imageUrl}}" class="img-sm img-thumbnail" alt="Item">
                                    </div>
                                    
                                </a>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-2 flex-grow-1 col-name text-align-center">
                                <a class="itemside" href="#">
                                    
                                    <div class="info">
                                        <h6 class="mb-0">{{this.category}}</h6>
                                    </div>
                                </a>
                            </div>
                            {{!-- <div class="col-lg-2 col-sm-2 col-4 col-price"> <span>$76.99</span> </div> --}}
                            
                            <div class="col-lg-1 col-sm-1 col-1 col-status">
                            {{#if this.isListed}}
                            <span class="badge rounded-pill alert-success">Listed</span>
                            {{else}}
                            <span class="badge rounded-pill alert-danger">Unlisted</span>
                            {{/if}}
                            </div>
                            <div class="col-lg-1 col-sm-1 col-1 col-date text-end">
                                <a href="/admin/edit_category/{{this._id}}" onclick="return confirm ('Are you sure want to edit?')" class="btn btn-pill font-sm rounded btn-brand">
                                    <i class="material-icons md-edit"></i> Edit
                                </a>
                            </div>
                            {{#if this.isListed}}
                            <div class="col-lg-1 col-sm-1 col-1 col-date text-end">
                                <a href="/admin/delete_category/{{this._id}}" onclick="return confirm ('Are you sure want to Unlist?')" class="btn btn-pill font-sm rounded btn-brand">
                                    <i class="material-icons md-edit"></i>UnList
                                </a>
                            </div>
                            {{else}}
                            <div class="col-lg-1 col-sm-1 col-1 col-date text-end">
                                <a href="/admin/delete_category/{{this._id}}" onclick="return confirm ('Are you sure want to list?')" class="btn btn-pill font-sm rounded btn-brand">
                                    <i class="material-icons md-edit"></i>List
                                </a>
                            </div>
                            {{/if}}
                        </div> <!-- row .// -->
                    </article> 
                    {{/each}}
                    <!---end category tab-->
               


                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation example">
